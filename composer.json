{
    "name": "orchestra/platform",
    "description": "Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/",
    "keywords": ["framework", "laravel", "orchestra-platform", "orchestral"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.3",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3.1",
        "laravel/tinker": "^2.0",
        "orchestra/avatar": "^6.0",
        "orchestra/control": "^6.0",
        "orchestra/foundation": "^6.0",
        "orchestra/installer": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "orchestra/testing": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "classmap": [
            "database/factories",
            "database/seeds"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "config-cache": []
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Orchestra\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan orchestra:assemble"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
